{
    "manifest_version": 2,
    "name": "api-helper",
    "version": "1.2.1",
    "description": "api处理器",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html"
}
